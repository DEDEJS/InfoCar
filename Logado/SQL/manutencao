CREATE TABLE `infocar`.`maintenance` (`id_manutencao` INT(10) NOT NULL AUTO_INCREMENT ,`observacao` varchar(300) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL ,`peca` VARCHAR(50) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL , `valor_peca` INT(10) NOT NULL , `valor_mao_de_obra` VARCHAR(10) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL , `data` DATE NOT NULL , `tipo_de_manutencao` VARCHAR(50) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL , `local` VARCHAR(50) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL , PRIMARY KEY (`id_manutencao`)) ENGINE = InnoDB;
ALTER TABLE `maintenance` CHANGE `peca` `Peca` VARCHAR(50) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL, CHANGE `valor_peca` `Valor_Peca` INT(10) NOT NULL, CHANGE `valor_mao_de_obra` `Valor_Mao_De_Obra` VARCHAR(10) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL, CHANGE `data` `Data` DATE NOT NULL, CHANGE `tipo_de_manutencao` `Tipo_De_Manutencao` VARCHAR(50) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL, CHANGE `local` `Local` VARCHAR(50) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL;
